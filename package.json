{
  "name": "boilerplate-react-redux",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --hot --inline --history-api-fallback --host 0.0.0.0 & npm run stub",
    "stub": "node stub",
    "build": "webpack --config webpack.prod.config -p",
    "test": "NODE_PATH=$(pwd)/src jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "eslint": "eslint 'src/**/*.{js,jsx}' --ignore-pattern *.spec.js --quiet",
    "pull-request-validator": "npm run eslint && npm run test"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "babel-cli": "^6.24.1",
    "babel-core": "6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.22.0",
    "css-loader": "0.26.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-redux-saga": "^0.3.0",
    "file-loader": "0.9.0",
    "hapi": "^16.5.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.1",
    "joi": "^10.2.2",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.4.2",
    "react-dnd-test-backend": "^2.4.0",
    "react-intl": "^2.2.3",
    "redux-raven-middleware": "^1.2.0",
    "regenerator": "^0.9.7",
    "sass-loader": "^6.0.6",
    "sinon": "2.0.0-pre.5",
    "style-loader": "0.13.1",
    "webdriver-manager": "^12.0.5",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "rc-progress": "^2.2.2",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-svg": "^2.1.21",
    "react-text-mask": "^2.3.1",
    "react-transition-group": "^1.2.0",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "redux-saga": "^0.15.3",
    "regenerator-runtime": "^0.10.5",
    "styled-components": "^2.3.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/core/unit-test",
      "actions.js"
    ],
    "setupFiles": [
      "<rootDir>/src/core/unit-test/setup.js"
    ],
    "testRegex": "/__tests__/.*.spec.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg|png)$": "<rootDir>/src/core/unit-test/jest-stub.js",
      "^mocks(.*)$": "<rootDir>/stub"
    },
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
